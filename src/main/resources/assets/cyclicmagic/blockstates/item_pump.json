{
    "multipart": [
        {
            "when": { "facing": "up" },
            "apply": { "model": "cyclicmagic:item_pump"}
        },
        {
            "when": { "facing": "down" },
            "apply": { "model": "cyclicmagic:item_pump", "x":180}
        },
        {
            "when": { "facing": "north" },
            "apply": { "model": "cyclicmagic:item_pump", "x":90 }
        },
        {
            "when": { "facing": "south" },
            "apply": { "model": "cyclicmagic:item_pump", "x":90, "y":180}
        },
        {
            "when": { "facing": "east" },
            "apply": { "model": "cyclicmagic:item_pump", "x":90, "y":90 }
        },
        {
            "when": { "facing": "west" },
            "apply": { "model": "cyclicmagic:item_pump", "x":270,"y":90 }
        }
		,
		
        {
            "when": {  "down": "cable"  },
            "apply": { "model": "cyclicmagic:cable_side_item", "x": 90, "uvlock": true }
        },
        {
            "when": { "up": "none" },
            "apply": { "model": "cyclicmagic:cable_none_item", "x": 270, "uvlock": true }
        },
        
        {
            "when": {   "up": "cable"   },
            "apply": { "model": "cyclicmagic:cable_side_item", "x": 270, "uvlock": true }
        },
        {
            "when": { "north": "none" },
            "apply": { "model": "cyclicmagic:cable_none_item" }
        },
      
        {
            "when": {   "north": "cable"  },
            "apply": { "model": "cyclicmagic:cable_side_item" }
        },
        {
            "when": { "south": "none" },
            "apply": { "model": "cyclicmagic:cable_none_item", "y": 180, "uvlock": true }
        },
      
        {
            "when": {  "south": "cable"  },
            "apply": { "model": "cyclicmagic:cable_side_item", "y": 180, "uvlock": true }
        },
        {
            "when": { "west": "none" },
            "apply": { "model": "cyclicmagic:cable_none_item", "y": 270, "uvlock": true }
        },
       
        {
            "when": {   "west": "cable"  },
            "apply": { "model": "cyclicmagic:cable_side_item", "y": 270, "uvlock": true }
        },
        {
            "when": { "east": "none" },
            "apply": { "model": "cyclicmagic:cable_none_item", "y": 90, "uvlock": true }
        },
       
        {
            "when": {   "east": "cable"  },
            "apply": { "model": "cyclicmagic:cable_side_item", "y": 90, "uvlock": true }
        },
		
			  
     	  
        {
            "when": { "east": "blocked" },
            "apply": { "model": "cyclicmagic:cable_blocked", "y": 90, "uvlock": true }
        },
        {
            "when": { "west": "blocked" },
            "apply": { "model": "cyclicmagic:cable_blocked", "y": 270, "uvlock": true }
        },
        {
            "when": { "south": "blocked" },
            "apply": { "model": "cyclicmagic:cable_blocked", "y": 180, "uvlock": true }
        },
        {
            "when": { "north": "blocked" },
            "apply": { "model": "cyclicmagic:cable_blocked" }
        },
        {
            "when": { "up": "blocked" },
            "apply": { "model": "cyclicmagic:cable_blocked", "x": 270, "uvlock": true }
        },
        {
            "when": { "down": "blocked" },
            "apply": { "model": "cyclicmagic:cable_blocked", "x": 90, "uvlock": true }
        }
		
    ]
}
