{
    "multipart": [
        {
            "when": { "facing": "up" },
            "apply": { "model": "cyclicmagic:fluid_pump"}
        },
        {
            "when": { "facing": "down" },
            "apply": { "model": "cyclicmagic:fluid_pump", "x":180}
        },
        {
            "when": { "facing": "north" },
            "apply": { "model": "cyclicmagic:fluid_pump", "x":90 }
        },
        {
            "when": { "facing": "south" },
            "apply": { "model": "cyclicmagic:fluid_pump", "x":90, "y":180}
        },
        {
            "when": { "facing": "east" },
            "apply": { "model": "cyclicmagic:fluid_pump", "x":90, "y":90 }
        },
        {
            "when": { "facing": "west" },
            "apply": { "model": "cyclicmagic:fluid_pump", "x":270,"y":90 }
        }
		,
        {
            "when": { "down": "none" },
            "apply": { "model": "cyclicmagic:cable_none_fluid", "x": 90, "uvlock": true }
        },
   
        {
            "when": { "OR": [ { "down": "cable" }, { "down": "inventory" } ] },
            "apply": { "model": "cyclicmagic:cable_side_fluid", "x": 90, "uvlock": true }
        },
        {
            "when": { "up": "none" },
            "apply": { "model": "cyclicmagic:cable_none_fluid", "x": 270, "uvlock": true }
        },
    
        {
            "when": { "OR": [ { "up": "cable" }, { "up": "inventory" } ] },
            "apply": { "model": "cyclicmagic:cable_side_fluid", "x": 270, "uvlock": true }
        },
        {
            "when": { "north": "none" },
            "apply": { "model": "cyclicmagic:cable_none_fluid" }
        },
      
        {
            "when": { "OR": [ { "north": "cable" }, { "north": "inventory" } ] },
            "apply": { "model": "cyclicmagic:cable_side_fluid" }
        },
        {
            "when": { "south": "none" },
            "apply": { "model": "cyclicmagic:cable_none_fluid", "y": 180, "uvlock": true }
        },
       
        {
            "when": { "OR": [ { "south": "cable" }, { "south": "inventory" } ] },
            "apply": { "model": "cyclicmagic:cable_side_fluid", "y": 180, "uvlock": true }
        },
        {
            "when": { "west": "none" },
            "apply": { "model": "cyclicmagic:cable_none_fluid", "y": 270, "uvlock": true }
        },
      
        {
            "when": { "OR": [ { "west": "cable" }, { "west": "inventory" } ] },
            "apply": { "model": "cyclicmagic:cable_side_fluid", "y": 270, "uvlock": true }
        },
        {
            "when": { "east": "none" },
            "apply": { "model": "cyclicmagic:cable_none_fluid", "y": 90, "uvlock": true }
        },
      
        {
            "when": { "OR": [ { "east": "cable" }, { "east": "inventory" } ] },
            "apply": { "model": "cyclicmagic:cable_side_fluid", "y": 90, "uvlock": true }
        }
		
    ]
}
