{
    "__comment": "Copyright © InsomniaKitten 2017",
    "multipart": [
        {
            "when": { "down": "none" },
            "apply": { "model": "cyclic:block/cable_none_fluid", "x": 90, "uvlock": true }
        },
        {
            "when": { "down": "blocked" },
            "apply": { "model": "cyclic:block/cable_blocked", "x": 90, "uvlock": true }
        },
        {
            "when": { "down": "inventory" },
            "apply": { "model": "cyclic:block/cable_side_fluid", "x": 90, "uvlock": true }
        },
        {
            "when": { "up": "none" },
            "apply": { "model": "cyclic:block/cable_none_fluid", "x": 270, "uvlock": true }
        },
        {
            "when": { "up": "blocked" },
            "apply": { "model": "cyclic:block/cable_blocked", "x": 270, "uvlock": true }
        },
        {
            "when": { "up": "inventory" } ,
            "apply": { "model": "cyclic:block/cable_side_fluid", "x": 270, "uvlock": true }
        },
        {
            "when": { "north": "none" },
            "apply": { "model": "cyclic:block/cable_none_fluid" }
        },
        {
            "when": { "north": "blocked" },
            "apply": { "model": "cyclic:block/cable_blocked" }
        },
        {
            "when": { "north": "inventory" } ,
            "apply": { "model": "cyclic:block/cable_side_fluid" }
        },
        {
            "when": { "south": "none" },
            "apply": { "model": "cyclic:block/cable_none_fluid", "y": 180, "uvlock": true }
        },
        {
            "when": { "south": "blocked" },
            "apply": { "model": "cyclic:block/cable_blocked", "y": 180, "uvlock": true }
        },
        {
            "when": { "south": "inventory" } ,
            "apply": { "model": "cyclic:block/cable_side_fluid", "y": 180, "uvlock": true }
        },
        {
            "when": { "west": "none" },
            "apply": { "model": "cyclic:block/cable_none_fluid", "y": 270, "uvlock": true }
        },
        {
            "when": { "west": "blocked" },
            "apply": { "model": "cyclic:block/cable_blocked", "y": 270, "uvlock": true }
        },
        {
            "when": { "west": "inventory" } ,
            "apply": { "model": "cyclic:block/cable_side_fluid", "y": 270, "uvlock": true }
        },
        {
            "when": { "east": "none" },
            "apply": { "model": "cyclic:block/cable_none_fluid", "y": 90, "uvlock": true }
        },
        {
            "when": { "east": "blocked" },
            "apply": { "model": "cyclic:block/cable_blocked", "y": 90, "uvlock": true }
        },
        {
            "when":  { "east": "inventory" } ,
            "apply": { "model": "cyclic:block/cable_side_fluid", "y": 90, "uvlock": true }
        },
        {
            "when": { "extract": "east" },
            "apply": { "model": "cyclic:block/cable_pump_fluid", "y": 90, "uvlock": true }
        },
        {
            "when": { "extract": "down" },
            "apply": { "model": "cyclic:block/cable_pump_fluid", "x": 90, "uvlock": true }
        },
        {
            "when": { "extract": "up" },
            "apply": { "model": "cyclic:block/cable_pump_fluid", "x": 270, "uvlock": true }
        },
        {
            "when": { "extract": "north" },
            "apply": { "model": "cyclic:block/cable_pump_fluid" }
        },
        {
            "when": { "extract": "west" },
            "apply": { "model": "cyclic:block/cable_pump_fluid", "y": 270, "uvlock": true }
        },
        {
            "when": { "extract": "south" },
            "apply": { "model": "cyclic:block/cable_pump_fluid", "y": 180, "uvlock": true }
        }
    ]
}
